<?xml version="1.0" encoding="UTF-8"?>
<module rename-to='GWT'>
    <!-- Inherit the core Web Toolkit stuff.                        -->
    <inherits name='com.google.gwt.user.User'/>

    <!-- We need the JUnit module in the main module,               -->
    <!-- otherwise eclipse complains (Google plugin bug?)           -->
    <inherits name='com.google.gwt.junit.JUnit'/>

    <!-- Inherit the default GWTEntryPoint style sheet.  You can change       -->
    <!-- the theme of your GWTEntryPoint application by uncommenting          -->
    <!-- any one of the following lines.                            -->
    <inherits name='com.google.gwt.user.theme.standard.Standard'/>
    <!-- <inherits name='com.google.gwt.user.theme.chrome.Chrome'/> -->
    <!-- <inherits name='com.google.gwt.user.theme.dark.Dark'/>     -->

    <!-- Other module inherits                                      -->
  	 <inherits name="thothbot.parallax.core.Core"/>
  	 <inherits name="thothbot.parallax.plugins.effects.Effects"/>
  
  	 <!--   Logging -->
    <inherits name="com.google.gwt.logging.Logging"/>
    <set-property name="gwt.logging.enabled" value="TRUE"/> 
    <set-property name="gwt.logging.logLevel" value="ALL"/>

    <set-property name="gwt.logging.consoleHandler" value="DISABLED" />
    <set-property name="gwt.logging.popupHandler" value="DISABLED" />

    <!-- Specify the app entry point class.                         -->
    <entry-point class='com.codenvy.example.gwt.client.GWTEntryPoint'/>

    <!-- Specify the paths for translatable code                    -->
    <source path='client'/>
    <source path='shared'/>

</module>
